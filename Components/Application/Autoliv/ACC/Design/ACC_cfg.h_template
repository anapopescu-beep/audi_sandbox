/**
 * \brief
 *       Accelerometer Safety Mechanisms Enable 0 Register from Bank 1
 *       Value to enable used Safety Mechanisms.
 */
/* ------------------------------------------------ */
/* Enable | Bit | SM number |  Name                 */
/* ------------------------------------------------ */
/*   1    |  14 |      SM08 | en_temp_sensecheck_sm */
/*   1    |  13 |      SM18 | en_accel_cp_sm        */
/*   1    |  12 |      SM17 | en_vrefshield_sm      */
/*   1    |  11 |      SM16 | en_accel_dcst_sm      */
/*   1    |  10 |      SM14 | en_vref_sm            */
/*   1    |   9 |      SM13 | en_vdd_sm             */
/*   1    |   8 |      SM12 | en_dvdd_sm            */
/*   1    |   7 |      SM11 | en_avdd_sm            */
/*   1    |   6 |      SM10 | en_vddmaster_sm       */
/*   0    |   4 |      SM04 | en_gyro_quad_adc_sm   */
/* ------------------------------------------------ */
#define KU16_ACC_SET_S_M_ENABLE_0_BK_1_USED_S_M                ((uint16)0x7FC0)

/**
 * \brief
 *       Accelerometer Safety Mechanisms Enable 1 Register from Bank 1
 *       Value to enable used Safety Mechanisms.
 */
/* ------------------------------------------------ */
/* Enable | Bit | SM number |  Name                 */
/* ------------------------------------------------ */
/*   0    |  12 |      SM36 | en_gyro_dcst_sm       */
/*   1    |   8 |      SM03 | en_drivefreq_meas_sm  */
/*   1    |   7 |      SM34 | en_bg_sm              */
/*   1    |   6 |      SM33 | en_regcheck_sm        */
/*   1    |   4 |      SM29 | en_ahbbus_sm          */
/* ------------------------------------------------ */
#define KU16_ACC_SET_S_M_ENABLE_1_BK_1_USED_S_M                ((uint16)0x01D0)

/**
 * \brief
 *       Accelerometer Safety Mechanisms Mask 0 Register from Bank 1
 *       Value to mask unused Safety Mechanisms.
 */
/* ----------------------------------------------------- */
/* Mask | Bit | SM number | Name                         */
/* ----------------------------------------------------- */
/*   1  |  13 |      SM04 | msk_gyro_z_quadadc_alarm     */
/*   1  |  12 |      SM04 | msk_gyro_y_quadadc_alarm     */
/*   1  |  11 |      SM04 | msk_gyro_x_quadadc_alarm     */
/*   0  |  10 |      SM03 | msk_rcosc1_freqmeas_alarm    */
/*   0  |   9 |      SM03 | msk_sys_clk_alarm            */
/*   1  |   8 |      SM03 | msk_gyro_z_drfreqmeas_alarm  */
/*   1  |   7 |      SM03 | msk_gyro_y_drfreqmeas_alarm  */
/*   1  |   6 |      SM03 | msk_gyro_x_drfreqmeas_alarm  */ 
/* ----------------------------------------------------- */
#define KU16_ACC_SET_S_M_MASK_0_BK_1_MASKED_S_M                ((uint16)0x39C0)

/**
 * \brief
 *       Accelerometer Safety Mechanisms Mask 1 Register from Bank 1
 *       Value to mask unused Safety Mechanisms.
 */
/* ----------------------------------------------------- */
/* Mask | Bit | SM number | Name                         */
/* ----------------------------------------------------- */
/*   0  |  12 |      SM14 | msk_temp_vref_alarm          */
/*   0  |  11 |      SM14 | msk_accel_vref_alarm         */
/*   1  |  10 |      SM14 | msk_gyro_vref_alarm          */
/*   0  |   9 |      SM13 | msk_vdd_alarm                */
/*   0  |   8 |      SM13 | msk_vddio_alarm              */
/*   0  |   7 |      SM12 | msk_dvddreg_alarm            */
/*   0  |   6 |      SM11 | msk_avddreg_alarm            */ 
/*   0  |   5 |      SM10 | msk_vddmaster_alarm          */ 
/*   0  |   4 |      SM08 | msk_temp12_alarm             */ 
/*   0  |   3 |      SM08 | msk_temp_dsp_alarm           */ 
/*   1  |   2 |      SM07 | msk_gyro_z_drclk_alarm       */ 
/*   1  |   1 |      SM07 | msk_gyro_y_drclk_alarm       */ 
/*   1  |   0 |      SM07 | msk_gyro_x_drclk_alarm       */ 
/* ----------------------------------------------------- */
#define KU16_ACC_SET_S_M_MASK_1_BK_1_MASKED_S_M                ((uint16)0x0407)

/**
 * \brief
 *       Accelerometer Safety Mechanisms Mask 3 Register from Bank 1
 *       Value to mask unused Safety Mechanisms.
 */
/* ----------------------------------------------------- */
/* Mask | Bit | SM number | Name                         */
/* ----------------------------------------------------- */
/*   1  |  15 |      SM36 | msk_gyro_z_dcst_alarm        */
/*   1  |  14 |      SM36 | msk_gyro_y_dcst_alarm        */
