/*   0  |   1 |      SM30 | msk_spi_cmddecod_alarm       */ 
/*   0  |   0 |      SM30 | msk_spi_crc_alarm            */ 
/* ----------------------------------------------------- */
#define KU16_ACC_SET_S_M_MASK_3_BK_1_MASKED_S_M                ((uint16)0xE000)

