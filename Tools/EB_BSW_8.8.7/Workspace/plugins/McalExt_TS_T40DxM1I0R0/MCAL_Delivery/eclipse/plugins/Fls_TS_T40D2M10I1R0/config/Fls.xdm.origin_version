                                            <a:a name="UUID" value="ECUC:efc12700-0636-47d6-9c1f-5d3d40bd6144"/>
                                            <a:a name="DEFAULT_RADIX" value="HEX" />
                                            <a:da name="DEFAULT" value="0"/>
                                            <a:a name="INVALID" type="Range">
                                                <a:tst expr="&gt;=0"/>
                                                <a:tst expr="&lt;=4294967295"/>
                                            </a:a>
                                        </v:var><!-- FlsSerialFlashB1Size -->

