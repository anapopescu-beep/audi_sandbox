                                                    <a:a name="EDITABLE" type="XPath" expr="(../../../../../../../MclGeneral/MclDma/MclEnableDma ='true') and (../../../../dmaLogicChannel_EnableGlobalConfig ='true')"/>
                                                                                                                                            <a:da name="DEFAULT" type="XPath" expr="(ecu:list('Mcl.MclConfigSet.DmaLevelPrio'))[position()-1=node:fallback(node:current()/../../../../@index, 0)]"/>
