                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v mclass="PostBuild">VariantPostBuild</icc:v>
                                        <icc:v mclass="PreCompile">VariantPreCompile</icc:v>
                                    </a:a>
                                    <v:var name="MclVirtualAddressStart" type="INTEGER">
                                        <a:a name="DESC">
                                            <a:v>
                                                <![CDATA[EN:<html>
                                                    This parameter represents the Virtual Address Start.
                                                    The address must be aligned to 4 bytes.
                                                    Note: Implementation Specific Parameter.
                                                </html>]]>
                                            </a:v>
                                        </a:a>
                                        <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                            <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                            <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                        </a:a>
                                        <a:a name="DEFAULT_RADIX" value="HEX"/>
                                        <a:a name="ORIGIN" value="NXP"/>
                                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                        <a:a name="LABEL" value="Virtual Address Start"/>
                                        <a:a name="UUID" value="ECUC:b37b9964-7528-11e8-adc0-fa7ae01bbebc"/>
                                        <a:da name="DEFAULT" value="0"/>
                                        <a:da name="INVALID" type="Range">
                                            <a:tst expr="&lt;=4294967295"/>
                                            <a:tst expr="&gt;=0"/>
                                        </a:da>
                                        <a:da name="RANGE" type="XPath">
                                            <a:tst expr="(node:fallback(., 0) mod 4) != 0" true="The Virtual Address Start must be 4 byte alligned."/>
                                        </a:da>
                                    </v:var>

                                    <v:var name="MclVirtualAddressEnd" type="INTEGER">
                                        <a:a name="DESC">
                                            <a:v>
                                                <![CDATA[EN:<html>
                                                    This parameter represents the Virtual Address End.
                                                    The address must be aligned to 4 bytes.
                                                    Note: Implementation Specific Parameter.
                                                </html>]]>
                                            </a:v>
                                        </a:a>
                                        <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                            <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                            <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                        </a:a>
                                        <a:a name="DEFAULT_RADIX" value="HEX" />
                                        <a:a name="ORIGIN" value="NXP"/>
                                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                        <a:a name="LABEL" value="Virtual Address End"/>
                                        <a:a name="UUID" value="ECUC:b37b9aa4-7528-11e8-adc0-fa7ae01bbebc"/>
                                        <a:da name="DEFAULT" value="255"/>
                                        <a:da name="INVALID" type="Range">
                                            <a:tst expr="&lt;=4294967295"/>
                                            <a:tst expr="&gt;=0"/>
                                        </a:da>
                                        <a:da name="RANGE" type="XPath">
                                            <a:tst expr="((node:fallback(., 0)+1) mod 4) != 0" true="The Virtual Address end must be the last address of a page eg: a page with 64kb the end address should be like 0xxxxxFFFF in hexa format."/>
                                            <a:tst expr="(node:fallback(., 0) &lt; node:fallback(../MclVirtualAddressStart, 0))" true="The value of VirtualAddressEnd must be greater or equal VirtualAddressStart."/>
                                        </a:da>
                                    </v:var>

                                    <v:var name="MclPhysicalAddressStart" type="INTEGER">
                                        <a:a name="DESC">
                                            <a:v>
                                                <![CDATA[EN:<html>
                                                    This parameter represents the Physical Address Start.
                                                    The address must be aligned to 4 bytes.
                                                    Note: Implementation Specific Parameter.
                                                </html>]]>
                                            </a:v>
                                        </a:a>
                                        <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                            <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                            <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                        </a:a>
                                        <a:a name="DEFAULT_RADIX" value="HEX" />
                                        <a:a name="ORIGIN" value="NXP"/>
                                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                        <a:a name="LABEL" value="Physical Address Start"/>
                                        <a:a name="UUID" value="ECUC:145207b4-784e-11e8-adc0-fa7ae01bbebc"/>
                                        <a:da name="DEFAULT" value="0"/>
                                        <a:da name="INVALID" type="Range">
                                            <a:tst expr="&lt;=4294967295"/>
                                            <a:tst expr="&gt;=0"/>
                                        </a:da>
                                        <a:da name="RANGE" type="XPath">
                                            <a:tst expr="(node:fallback(., 0) mod 4) != 0" true="The Physical Address Start must be 4 byte alligned."/>
                                        </a:da>
                                    </v:var>

                                    <v:var name="MclPhysicalAddressEnd" type="INTEGER">
                                        <a:a name="DESC">
                                            <a:v>
                                                <![CDATA[EN:<html>
                                                    This parameter represents the Physical Address End.
                                                    The address must be aligned to 4 bytes.
                                                    Note: Implementation Specific Parameter.
                                                </html>]]>
                                            </a:v>
                                        </a:a>
                                        <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                            <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                            <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                        </a:a>
                                        <a:a name="DEFAULT_RADIX" value="HEX" />
                                        <a:a name="ORIGIN" value="NXP"/>
                                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                        <a:a name="LABEL" value="Physical Address End"/>
                                        <a:a name="UUID" value="ECUC:14520a3e-784e-11e8-adc0-fa7ae01bbebc"/>
                                        <a:da name="DEFAULT" value="255"/>
                                        <a:da name="INVALID" type="Range">
                                            <a:tst expr="&lt;=4294967295"/>
                                            <a:tst expr="&gt;=0"/>
                                        </a:da>
                                        <a:da name="RANGE" type="XPath">
                                            <a:tst expr="((node:fallback(., 0)+1) mod 4) != 0" true="The Physical Address end must be the last address of a page eg: a page with 64kb the end address should be like 0xxxxxFFFF in hexa format."/>
                                            <a:tst expr="(node:fallback(., 0) &lt; node:fallback(../MclPhysicalAddressStart, 0))" true="The value of PhysicalAddressEnd must be greater or equal PhysicalAddressStart."/>
                                            <a:tst expr="( (num:i(number(node:fallback(../MclVirtualAddressEnd, 0))) - num:i(number(node:fallback(../MclVirtualAddressStart, 0))))!= (num:i(number(node:fallback(., 0))) - num:i(number(node:fallback(../MclPhysicalAddressStart, 0)))) )" true="PhysicalAddressEnd - PhysicalAddressStart must be equal VirtualAddressEnd - VirtualAddressStart ."/>
                                        </a:da>
                                    </v:var>
                                </v:ctr>
                            </v:lst>

                                                    <v:ctr name="MclDemEventParameterRefs" type="IDENTIFIABLE">
                                <a:a name="TAB" value="MclDemEventParameterRefs"/>
                                <a:a name="DESC">
                                    <a:v>
                                        <![CDATA[EN:<html>
                                            Container for the references to DemEventParameter elements which shall be invoked using the API Dem_ReportErrorStatus API in case the corresponding error occurs. <p/>
                                            The EventId is taken from the referenced DemEventParameter&apos;s DemEventId value.
                                            The standardized errors are provided in the container and can be extended by vendor specific error references.<p/>
                                        </html>]]>
                                    </a:v>
                                </a:a>
                                <a:a name="OPTIONAL" value="true"/>
                                <a:a name="UUID" value="ECUC:2e691b22-e0ab-4ca3-b254-93d66ec568c9"/>
                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                    <icc:v mclass="PreCompile">VariantPreCompile</icc:v>
                                    <icc:v mclass="PreCompile">VariantPostBuild</icc:v>
                                </a:a>
                                <a:a name="EDITABLE" type="XPath">
                                    <a:tst expr="../../MclGeneral/MclEnableDemReportErrorStatus ='true'"/>
                                </a:a>
                                <v:ref name="MCL_E_TIMEOUT_FAILURE" type="SYMBOLIC-NAME-REFERENCE">
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:<html>
                                                Reference to configured DEM event to report Timeout failure.<p/>
                                            </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PreCompile">VariantPostBuild</icc:v>
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="NXP"/>
                                    <a:a name="UUID" value="ECUC:e5f7b7e8-5cf6-4c20-9488-da0d5a2041f0"/>
                                    <a:a name="OPTIONAL" value="true"/>
                                    <a:a name="SCOPE" value="LOCAL"/>
                                    <a:da name="REF" value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/Dem/DemConfigSet/DemEventParameter"/>
                                    <a:da name="INVALID" type="XPath">
                                         <a:tst expr="node:refvalid(.)" false="Invalid or empty reference."/>
                                    </a:da>
                                    <a:a name="EDITABLE" type="XPath">
                                        <a:tst expr="../../../MclGeneral/MclEnableDemReportErrorStatus ='true'"/>
                                    </a:a>
                                </v:ref>
                            </v:ctr>
                        
