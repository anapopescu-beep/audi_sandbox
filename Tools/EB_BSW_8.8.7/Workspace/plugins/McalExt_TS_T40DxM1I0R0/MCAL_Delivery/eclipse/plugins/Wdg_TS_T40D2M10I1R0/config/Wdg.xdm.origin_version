                                            </a:da>
                                            <a:da name="RANGE" type="XPath">
                                                <a:tst expr="(../WdgWindowMode='true') and (node:value(.) &gt; ((255 * (node:value(../WdgPrescalerValue) + 1)) div (node:value(../WdgClockValue) * 1000))) " true="Compare Low Register Value must be less than 256, please decrease clock source frequency provided for EWM, or increase WdgPrescalerValue value or descrease the value of WdgTimeoutPeriod."/>
                                                <a:tst expr="(../WdgWindowMode='true') and (node:value(.) &gt;= node:value(../WdgTimeoutPeriod)) " true="The WdgWindowPeriod must be less than the WdgTimeoutPeriod."/>
                                            </a:da>
                                        </v:var>
                                    </v:ctr>
                                </v:ctr>
