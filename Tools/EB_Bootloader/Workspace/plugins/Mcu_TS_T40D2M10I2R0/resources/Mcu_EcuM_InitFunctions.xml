<?xml version="1.0" encoding="UTF-8"?>
<ecum:initFunctions
    xmlns:ecum="http://www.tresos.de/_project/AutoCore/ecum.initFunctions_1.xsd"
    xmlns:svc="http://www.tresos.de/_project/AutoCore/svc.base_1.xsd"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.tresos.de/_project/AutoCore/ecum.initFunctions_1.xsd">

    <initFunction name="Mcu_Init_PC">
        <enable type="XPATH" value="as:modconf('Mcu')[1]/IMPLEMENTATION_CONFIG_VARIANT = 'VariantPreCompile'"/>
        <moduleId value="Mcu" />
        <service value="Init" />
        <initList value="1" />
        <initConfigStr value ="NULL_PTR"/>
        <initializeAfter>
            <moduleId>Dem</moduleId>
        </initializeAfter>
        <initializeBefore>
            <moduleId>MyMcuArchClockInit</moduleId>
            <moduleId>Eb_Intgr_McuArchClockInit</moduleId>
            <moduleId>Mcl</moduleId>
            <moduleId>Port</moduleId>
            <moduleId>Dio</moduleId>
            <moduleId>Gpt</moduleId>
            <moduleId>Wdg</moduleId>
            <moduleId>WdgM</moduleId>
            <moduleId>Adc</moduleId>
            <moduleId>Icu</moduleId>
            <moduleId>Pwm</moduleId>
        </initializeBefore>
    </initFunction>

    <initFunction name="Mcu_Init_PB">
        <enable type="XPATH" value="as:modconf('Mcu')[1]/IMPLEMENTATION_CONFIG_VARIANT = 'VariantPostBuild'"/>
        <moduleId value="Mcu" />
        <service value="Init" />
        <initList value="1" />
        <moduleConfigurationRef type="XPATH" value="as:modconf('Mcu')[1]/McuModuleConfiguration" />
        <initializeAfter>
            <moduleId>Dem</moduleId>
        </initializeAfter>
        <initializeBefore>
            <moduleId>MyMcuArchClockInit</moduleId>
            <moduleId>Eb_Intgr_McuArchClockInit</moduleId>
            <moduleId>Mcl</moduleId>
            <moduleId>Port</moduleId>
            <moduleId>Dio</moduleId>
            <moduleId>Gpt</moduleId>
            <moduleId>Wdg</moduleId>
            <moduleId>WdgM</moduleId>
            <moduleId>Adc</moduleId>
            <moduleId>Icu</moduleId>
            <moduleId>Pwm</moduleId>
        </initializeBefore>
    </initFunction>

</ecum:initFunctions>
