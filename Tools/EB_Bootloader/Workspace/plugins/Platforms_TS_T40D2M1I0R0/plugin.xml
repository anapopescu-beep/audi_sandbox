<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
  <!--
  ** AutoCore Platforms_TS_T40D2M1I0R0 plugin
  **
  ** All rights exclusively reserved for Elektrobit Automotive GmbH,
  ** unless expressly agreed to otherwise.
  -->
  
  <extension point="dreisoft.tresos.launcher2.plugin.module"
             id="Platforms_TS_T40D2M1I0R0_ModuleId"
             name="Platforms_TS_T40D2M1I0R0 Module">

    <module id="Platforms_TS_T40D2M1I0R0"
            label="Platforms"
            mandatory="true"
            allowMultiple="false"
            description="ACM-Base: Platform extensions for application build environment"
            copyright="(c) 2005-2019 Elektrobit Automotive GmbH"
            swVersionMajor="1"
            swVersionMinor="0"
            swVersionPatch="0"
            swVersionSuffix="B280288"
            specVersionMajor="0"
            specVersionMinor="0"
            specVersionPatch="0"
            specVersionSuffix="0000"
            relVersionMajor="4"
            relVersionMinor="0"
            relVersionPatch="3"
            categoryType="Platforms"
            categoryLayer="EB Base Bundle"
            categoryCategory="EB"
            categoryComponent="ECUC">

      <ecuType target="CORTEXM" derivate="S32K14X"/>
      
    </module>
  </extension>


  <!-- Extensions for registering the platforms resource files -->
  <extension point="dreisoft.tresos.autosar2.api.plugin.ecuresource">
    <ecuresource
      target="CORTEXM"
      derivate="S32K14X"
      moduleId="Platforms_TS_T40D2M1I0R0"
      file="resource/CORTEXM_S32K14X.properties">
    </ecuresource>
  </extension>

  <extension point="dreisoft.tresos.autosar2.api.plugin.ecuresourcefinder">
    <ecuresourcefinder>
      <finder class="dreisoft.tresos.autosar2.api.ecuresource.DefaultEcuResourceFinder">
      </finder>
    </ecuresourcefinder>
  </extension>

  <extension point="dreisoft.tresos.launcher2.plugin.configuration"
             id="Platforms_TS_T40D2M1I0R0_ConfigId"
             name="Platforms_TS_T40D2M1I0R0 Configuration">
    <configuration moduleId="Platforms_TS_T40D2M1I0R0">
      <schema>
        <manager class="dreisoft.tresos.autosar2.resourcehandling.AutosarSchemaManager"/>
          <resource value="config/Platforms.xdm" type="xdm"/>
      </schema>

      <data>
        <manager class="dreisoft.tresos.autosar2.resourcehandling.AutosarConfigManager"/>
        <schemaNode path="ASPath:/TS_T40D2M1I0R0/Platforms"/>
      </data>

      <editor id="Platforms_TS_T40D2M1I0R0_EditorId"
              label="Default"
              tooltip="Base BSW module">
        <class class="dreisoft.tresos.launcher2.editor.GenericConfigEditor">
          <parameter name="schema" value="ASPath:/TS_T40D2M1I0R0/Platforms"/>
          <parameter name="title" value="Platforms"/>
          <parameter name="noTabs" value="false"/>
          <parameter name="noLinks" value="true"/>
          <parameter name="groupLinks" value="false"/>
          <parameter name="groupContainers" value="false"/>
          <parameter name="groupTables" value="true"/>
          <parameter name="optionalGeneralTab" value="true"/>
        </class>
      </editor>
    </configuration>
  </extension>

</plugin>

