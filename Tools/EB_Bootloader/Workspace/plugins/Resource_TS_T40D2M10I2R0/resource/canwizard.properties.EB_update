# --- S32K14x specific configuration ---
# - 3 CAN controllers(FlexCAN) with 32/16/16 message buffers
# - Each message buffer has its own filter mask
# - Don't care bit is 0

CanWiz.MCUConfig.SharedFilterMasks:0
CanWiz.MCUConfig.DontCareBitValue:false
CanWiz.MCUConfig.NumberOfCCs:3
CanWiz.MCUConfig.ControllerConfigs:CanWiz.FlexCAN_A,CanWiz.FlexCAN_B,CanWiz.FlexCAN_C
CanWiz.MCUConfig.SystemClocks:CanWiz.SystemClock1,CanWiz.SystemClock2
CanWiz.MCUConfig.SharedHOHGroups:

# FlexCAN_A controller
# Each message buffer has its own filter mask
CanWiz.FlexCAN_A.SharedFilterMasks:0
CanWiz.FlexCAN_A.SharedHOHGroups:CanWiz.FlexCAN_A_32HOH
CanWiz.FlexCAN_A.BrpMin:1
CanWiz.FlexCAN_A.BrpMax:8

CanWiz.FlexCAN_A_32HOH.Name:CAN_A_32HOH
CanWiz.FlexCAN_A_32HOH.FilterMask:own
CanWiz.FlexCAN_A_32HOH.Direction:both
CanWiz.FlexCAN_A_32HOH.NumberOfHandles:32

# FlexCAN_B controller
# Each message buffer has its own filter mask
CanWiz.FlexCAN_B.SharedFilterMasks:0
CanWiz.FlexCAN_B.SharedHOHGroups:CanWiz.FlexCAN_B_16HOH
CanWiz.FlexCAN_B.BrpMin:1
CanWiz.FlexCAN_B.BrpMax:8

CanWiz.FlexCAN_B_16HOH.Name:CAN_B_16HOH
CanWiz.FlexCAN_B_16HOH.FilterMask:own
CanWiz.FlexCAN_B_16HOH.Direction:both
CanWiz.FlexCAN_B_16HOH.NumberOfHandles:16

# FlexCAN_C controller
# Each message buffer has its own filter mask
CanWiz.FlexCAN_C.SharedFilterMasks:0
CanWiz.FlexCAN_C.SharedHOHGroups:CanWiz.FlexCAN_C_16HOH
CanWiz.FlexCAN_C.BrpMin:1
CanWiz.FlexCAN_C.BrpMax:8

CanWiz.FlexCAN_C_16HOH.Name:CAN_C_16HOH
CanWiz.FlexCAN_C_16HOH.FilterMask:own
CanWiz.FlexCAN_C_16HOH.Direction:both
CanWiz.FlexCAN_C_16HOH.NumberOfHandles:16
